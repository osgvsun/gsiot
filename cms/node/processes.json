{
    "apps": [{
        "name": "Nodejs CMS", //名称
        "script": "./app.js", //程序入库
        "cwd": "./", //根目录
        "watch": [
            "webroot",
            "services",
            "router"
        ], //需要监控的目录
        "error_file": "./logs/cms_err.log", //错误输出日志
        "out_file": "./logs/cms_out.log", //日志
	"merge_logs"      : true,
 	"log_date_format" : "YYYY-MM-DD HH:mm Z"
    }]
}
