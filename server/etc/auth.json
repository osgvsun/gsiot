{
    "device":{
        "mode":"touchoff",
        "runlog":"/etc/log/attlog.txt",
        "help":"mode代表使用卡的方式，touchoff是触发式,resident是驻留式",
        "datasource": {
            "x-datasource": "limsproduct"
        }
    },
    "fromcard_username": {
        "mode": "local",
        "checkpower": "no",
        "authfromiotserver": true,
        "policyfile": "/etc/conf/cardpolicy.json",
        "userbuff": "/etc/conf/userbuff.json",
        "iotwebservice": {
            "flag":true,
            "fromusername": "/agent/getusername/{}",
            "fromcard": "/agent/getcard/{}"
        },
        "searchuser": "webservice",
        "webservice": {
            "flag":true,
            "card": "http://{}:{}/api/usercenter/share/getUserByNumber?cardNo={}",
            "username": "http://{}:{}/api/usercenter/share/getUserByNumber?employeeNo={}",
            "header": {
                "x-datasource": "limsproduct"
            }
        },
        "todo": {
            "searchfromruntime": "",
            "output": "todo"
        }
    },
    "fromlogin": {
        "mode":"webservice",
        "iotwebservice": {
            "fromusername": "/login/<username>/<passwd>",
            "fromphone": "/login/<phone>/<passwd>"
        },
        "webservice": {
            "passwd":"http://{}:{}/uaa/apiForMiniProgram/loginByUsernameAndPassword?username={}&password={}",
            "phone":"http://{}:{}/uaa/apiForMiniProgram/loginByPhone?phone={}&password={}"
        }
    }
}